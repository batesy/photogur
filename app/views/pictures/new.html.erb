<h1>Add a Picture</h1>
<%= link_to "Cancel", pictures_url %>

<%= form_for @picture do |f| %>
  <p>
    <%= f.label "Artist" %><br>
    <%= f.text_field "artist" %>
  </p>
  <p>
    <%= f.label "Title" %><br>
    <%= f.text_field "title" %>
  </p>
  <p>
    <%= f.label "url" %><br>
    <%= f.text_field "url" %>
  </p>
  <p>
    <%= f.submit "Save" %>
  </p>
<% end %>