<h1>Edit Picture</h2>

<%= form_for @picture do |f| %>
  <p>
    <%= f.label "Artist" %><br>
    <%= f.text_field "artist" %>
  </p>
  <p>
    <%= f.label "Title" %><br>
    <%= f.text_field "title" %>
  </p>
  <p>
    <%= f.label "URL" %><br>
    <%= f.text_field "url" %>
  </p>
  <p>
    <%= f.submit "Save" %> <%= link_to "Cancel", picture_url %>
  </p>
<% end %>